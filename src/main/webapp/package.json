{
  "name": "staff-plan",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "prettier:check": "prettier --check \"src/**/*.{ts,html,scss,css}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,html,scss,css}\"",
    "compile:ts": "tsc --noEmit",
    "update": "ncu -i --format group"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.9.0",
  "dependencies": {
    "@angular/cdk": "21.1.3",
    "@angular/common": "21.1.3",
    "@angular/compiler": "21.1.3",
    "@angular/core": "21.1.3",
    "@angular/forms": "21.1.3",
    "@angular/platform-browser": "21.1.3",
    "@angular/router": "21.1.3",
    "@primeuix/themes": "2.0.3",
    "keycloak-js": "26.2.3",
    "primeicons": "7.0.0",
    "primeng": "21.1.1",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular/build": "21.1.3",
    "@angular/cli": "21.1.3",
    "@angular/compiler-cli": "21.1.3",
    "angular-eslint": "21.2.0",
    "eslint": "9.39.2",
    "jsdom": "28.0.0",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "vitest": "4.0.18"
  }
}
